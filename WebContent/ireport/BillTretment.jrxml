	<queryString>
		<![CDATA[SELECT drug.drug_name ,drug.drug_price,SUM(treatment_drug.drug_qty) AS drug_qty ,
		SUM(drug.drug_price*treatment_drug.drug_qty) AS total , other_treatment.other_name , 
		SUM(treatment_other.`other_prices`*other_treatment.`other_price`)AS totalother, SUM(treatment_other.other_prices) AS totalother_qty
		FROM drug ,treatment_drug ,treatment_other , other_treatment WHERE drug.id = treatment_drug.refer_drug_id 
		AND treatment_drug.refer_treatment_id = $P{id} AND treatment_other.other_id = other_treatment.id AND
		 treatment_other.refer_treatment_id = $P{id}GROUP BY drug.drug_name]]>
	</queryString>
