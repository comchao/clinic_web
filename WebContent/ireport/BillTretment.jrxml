	<queryString>
		<![CDATA[SELECT drug.drug_name, this_.drug_qty, this_.drug_sumprice
 FROM `treatment_drug` this_
 INNER JOIN drug ON drug.id = this_.refer_drug_id
WHERE this_.refer_treatment_id = $P{id}
 UNION
 SELECT other_treatment.other_name drug_name,  1 drug_qty, treatment_other.other_price
 FROM `treatment_other`
INNER JOIN other_treatment ON other_treatment.id = treatment_other.other_id
WHERE treatment_other.refer_treatment_id = $P{id}]]>
	</queryString>
